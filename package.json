{
  "name": "clone-tabnews",
  "version": "1.0.0",
  "description": "Implementação do https://www.tabnews.com.br para o https://curso.dev",
  "homepage": "https://github.com/igorliisboa/clone-tabnews#readme",
  "bugs": {
    "url": "https://github.com/igorliisboa/clone-tabnews/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/igorliisboa/clone-tabnews.git"
  },
  "license": "MIT",
  "author": "",
  "main": "index.js",
  "scripts": {
    "dev": "npm run service:up && next dev",
    "service:up": "docker compose -f infra/compose.yaml up -d",
    "service:down": "docker compose -f infra/compose.yaml down",
    "service:stop": "docker compose -f infra/compose.yaml stop",
    "build": "next build",
    "lint:check": "prettier --check .",
    "lint:fix": "prettier --write .",
    "test": "jest",
    "test:watch": "jest --watchAll",
    "migration:create": "node-pg-migrate -m infra/migrations create",
    "migration:up": "node-pg-migrate -m infra/migrations --envPath .env.development up"
  },
  "dependencies": {
    "dotenv": "^16.4.4",
    "next": "^16.1.6",
    "node-pg-migrate": "^6.2.2",
    "pg": "^8.11.3",
    "react": "^19.2.4",
    "react-dom": "^19.2.4"
  },
  "devDependencies": {
    "jest": "^29.6.2",
    "prettier": "^3.8.1"
  }
}
